<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Debug'">
    <ClCompile>
      <AdditionalOptions>%(AdditionalOptions)   -nologo -DWIN32 -D_WINDOWS -utf-8 -MP  -MDd -Z7 -Ob0 -Od -RTC1  </AdditionalOptions>
    </ClCompile>
    <Link>
      <AdditionalOptions>%(AdditionalOptions)  -machine:x64 -nologo    -debug -INCREMENTAL </AdditionalOptions>
    </Link>
    <Lib>
      <AdditionalOptions>%(AdditionalOptions) -machine:x64 -nologo</AdditionalOptions>
    </Lib>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Release'">
    <ClCompile>
      <AdditionalOptions>%(AdditionalOptions)   -nologo -DWIN32 -D_WINDOWS -utf-8 -MP  -MD -O2 -Oi -Gy -DNDEBUG -Z7  </AdditionalOptions>
    </ClCompile>
    <Link>
      <AdditionalOptions>%(AdditionalOptions)  -machine:x64 -nologo -DEBUG -INCREMENTAL:NO -OPT:REF -OPT:ICF   </AdditionalOptions>
    </Link>
    <Lib>
      <AdditionalOptions>%(AdditionalOptions) -machine:x64 -nologo</AdditionalOptions>
    </Lib>
  </ItemDefinitionGroup>
</Project>
